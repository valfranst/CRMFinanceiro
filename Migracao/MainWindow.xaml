<Window x:Class="Migracao.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Migracao"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid VerticalAlignment="Center" HorizontalAlignment="Center">
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal">
                <TextBlock x:Name="novoTxt" Text="Novo" Margin="0 0 100 0" />
                <TextBlock x:Name="antigoTxt" Text="Antigo" />
            </StackPanel>
            <Button Content="Iniciar" Click="btIniciar" Margin="0 20 0 0"/>
        </StackPanel>

    </Grid>
</Window>
